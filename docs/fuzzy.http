# GET /fuzzylogic
curl "http://localhost:8080/fuzzylogic" -H "Accept: application/json"
###
# POST /fuzzylogic (single step)
curl -sS -X POST "http://localhost:8080/fuzzylogic?displacement=0.1&velocity=0.5&acceleration=0.2" -H "Accept: application/json"
###
curl -sS -X POST "http://localhost:8080/fuzzylogic?displacement=0.10875937299926325&velocity=0.15893610963840293&acceleration=0.6091901117602654" -H "Accept: application/json"
###
# POST /fuzzylogic-batch (repeat query params for arrays)
curl -sS -X POST "http://localhost:8080/fuzzylogic-batch" -H "Accept: application/json" \
  -H "Content-Type: application/json" \
  -d '{"displacements":[0.1,0.15,0.2],"velocities":[0.5,0.55,0.6],"accelerations":[0.2,0.25,0.3]}'
